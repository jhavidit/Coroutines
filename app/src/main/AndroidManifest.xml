<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tech.jhavidit.coroutinesdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoroutinesDemo">
        <activity
            android:name=".view.errorHandling.ignoreErrorAndContinue.IgnoreErrorAndContinueActivity"
            android:exported="false" />
        <activity
            android:name=".view.errorHandling.exceptionHandler.ExceptionHandlerActivity"
            android:exported="false" />
        <activity
            android:name=".view.timeOut.TimeOutActivity"
            android:exported="false" />
        <activity
            android:name=".view.longRunningTwoTask.LongRunningTwoTaskActivity"
            android:exported="false" />
        <activity
            android:name=".view.longRunningOneTask.LongRunningOneTaskActivity"
            android:exported="false" />
        <activity
            android:name=".view.room.RoomDBActivity"
            android:exported="false" />
        <activity
            android:name=".view.seriesNetworkCall.SeriesNetworkCallActivity"
            android:exported="false" />
        <activity
            android:name=".view.parallelNetworkCall.ParallelNetworkCallActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.singleNetworkCall.SingleNetworkCallActivity"
            android:label="Single Network Activity" />
    </application>

</manifest>